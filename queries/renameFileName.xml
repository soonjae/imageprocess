<query id="renameFileName" action="update">
	<tables>
		<table name="files" />
	</tables>
	<columns>
		<column name="uploaded_filename" var="uploaded_filename" notnull="notnull" />
		<column name="direct_download" var="direct_download" notnull="notnull" />
	</columns>
	<conditions>
		<condition operation="equal" column="file_srl" var="file_srl" filter="number" notnull="notnull" />
	</conditions>
</query>
