<query id="updateFileSize" action="update">
	<tables>
		<table name="files" />
	</tables>
	<columns>
		<column name="file_size" var="file_size" notnull="notnull" />
	</columns>
	<conditions>
		<condition operation="equal" column="file_srl" var="file_srl" filter="number" notnull="notnull" />
	</conditions>
</query>
